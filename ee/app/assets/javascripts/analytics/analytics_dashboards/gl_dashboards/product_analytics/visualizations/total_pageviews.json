{
  "version": 1,
  "title": "Total pageviews",
  "type": "SingleStat",
  "data": {
    "type": "cube_analytics",
    "query": {
      "measures": [
        "TrackedEvents.pageViewsCount"
      ],
      "filters": [
        {
          "operator": "equals",
          "values": [
            "pageview"
          ],
          "member": "TrackedEvents.eventType"
        }
      ],
      "limit": 100,
      "timezone": "UTC",
      "dimensions": [

      ],
      "timeDimensions": [

      ]
    }
  },
  "options": {
  }
}
